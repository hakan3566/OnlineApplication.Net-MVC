@using OnlineApp.Models.Classes
@model List<SalesTransaction>
@{
    ViewBag.Title = "MyOrders";
    Layout = "~/Views/Shared/_CurrentLayout.cshtml";
}
<br />
<h2>My Orders History</h2>
<br />

<table class="table table-hover">
    <tr>
        <th>Product Name</th>
        <th>Price</th>
        <th>Date</th>
    </tr>
    @foreach (var x in Model)
    {
<tr>
    <td>@x.Product.ProductName</td>
    <td>@x.Price</td>
    <td>@Convert.ToDateTime(x.Date).ToString("dd/MM/yyyy")</td>
</tr>
    }

</table>